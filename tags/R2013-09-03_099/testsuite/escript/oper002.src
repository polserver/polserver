    print( "s1 < s2:    OK" );
else
    print( "s1 >= s2:   FAILED" );
endif

if (s1[1] < s2)
    print( "s1[1] < s2: OK" );
else
    print( "s1[1] < s2: FAILED" );
endif

if ("abcd" < "d")
    print( "abcd < d:   OK" );
else
    print( "abcd < d:   FAILED" );
endif

if (s1 < s2[1])
    print( "s1 < s2[1]: OK" );
else
    print( "s1 < s2[1]: FAILED" );
endif

