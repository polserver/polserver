program test()

	local startX := 3, endX := 8;
	local startY := 2, endY := 7;

	local fooY := 2;
	while (startX <= endX)
		while (startY <= endY)
			print( "startX=" +startX + ", startY=" + startY );
			startY := startY + 1;
		endwhile
		startY := fooY;
		startX := startX + 1;
		print( "startX=" + startX );
	endwhile

endprogram