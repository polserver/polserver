<?xml version="1.0" encoding="utf-8"?>
<Options>
  <!--LogPath is the default path to check for log files when you press the Parse Button-->
  <LogPath>C:\Logs</LogPath>
  <!--OutputPath is the default path to use when you press the Save Button after parsing-->
  <OutputPath>C:\Logs</OutputPath>
  <!--POLPath is the default path to use for the POL Parsing Tools in the Tools Menu-->
  <POLPath>C:\Pol</POLPath>
  <!--LastUsedList is the last Keyword List used when Options was last saved-->
  <LastUsedList>Test List</LastUsedList>
  <!--Case decides if to use Case Sensitive searches based on Keywords in the main Parser-->
  <Case>False</Case>
</Options>