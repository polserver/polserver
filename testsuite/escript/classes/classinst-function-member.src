// Tests the 'is' operator with class instances

class A()
  function A( this )
    print( $"A::A this={this}" );
  endfunction
endclass

class B()
  function B( this, args... )
    print( $"B::B this={this} args={args}" );
  endfunction
endclass

var a := A();
var b := B("args");

print( a.function );
print( b.function );

print("----");

var aa := ( a.function ).new();
var bb := b.function.new("foo","bar");

print( aa );
print( bb );