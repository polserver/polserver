class Foo()
  function Foo( this, name )
    this.name := name;
  endfunction

  function print( this, prefix )
    ::print( $"{prefix}: {this.name}!" );
  endfunction

  function set_member( this )
    this.print( "succeeded with method_id call" );
  endfunction
endclass

var obj := Foo( "NAME" );
obj.print( "PREFIX" );
obj.set_member();

