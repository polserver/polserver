var a := Boolean( 1 );
var b := Boolean( 0 );
print( a ); // true
print( b ); // false
print( pack( a ) );
print( pack( b ) );
print( unpack( pack( a ) ) ); // true
print( unpack( pack( b ) ) ); // false
print( typeof( a ) );
print( typeofint( a ) );
