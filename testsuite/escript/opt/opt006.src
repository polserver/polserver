// Ensure that boolean optimized expressions return same value as runtime
// calculations.

var T := true;
var F := false;
var U := uninit;

print( true && false );
print( T && F );

print( true || false );
print( T || F );

print( false && true );
print( F && T );

print( false || true );
print( F || T );

print( !false );
print( !F );

print( !true );
print( !T );

print( !uninit );
print( !U );
