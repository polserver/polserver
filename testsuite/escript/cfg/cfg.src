use cfgfile;
function get( cfg )
  print( cfg );
  foreach key in GetConfigStringKeys( cfg )
    var e := FindConfigElem( cfg, key );
    print( key );
    foreach entry in ListConfigElemProps( e )
      print( entry + " " + GetConfigString( e, entry )
           + " " + GetConfigString( e, entry ).find( "Ã¼" ) );
    endforeach
  endforeach
endfunction

