exported function load_container()
  var cnt:=CreateItemAtLocation(0,0,0,0x200001);
  if (!cnt)
    return ret_error("Failed to create container "+cnt);
  endif
  if (!cnt.no_drop_exception)
    DestroyItem(cnt);
    ret_error("Failed to load nodropexception");
  endif
  DestroyItem(cnt);
  return 1;
endfunction

