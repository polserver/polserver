use uo;
use os;

program caninsertscript( character, container, movetype, inserttype, adding_item, existing_stack, amount_to_add )
    GetProcess( GetObjProperty(container, "#TestPid") ).sendevent( struct{
        type := "container_caninsert",
        args := { character, container, movetype, inserttype, adding_item, existing_stack, amount_to_add }
    } );
endprogram
